          "escapeHTML": false,
          "type": "label",
          "size": "full-size",
          "icon": "Dice ",
          "value": "${actionLabel ? actionLabel : \"Use\"}$\n",
          "prefix": "",
          "suffix": "",
          "rollMessage": "(() => {\n  // In an itemContainer row: linkedEntity is the item.\n  // On an item sheet: entity is the item.\n  const doc = linkedEntity ?? entity;\n  const itemUuid = doc?.uuid;\n  if (!itemUuid) return ui.notifications?.warn(\"No item context found (linkedEntity/entity missing).\" );\n\n  const api = game.modules?.get(\"sinlesscsb\")?.api;\n  if (api?.castSpell && typeof api.castSpell === \"function\") {\n    return api.castSpell({ itemUuid });\n  }\n\n  const macro = game.macros.getName(\"Sinless Cast Spell\");\n  if (!macro) return ui.notifications?.warn(\"Macro 'Sinless Cast Spell' not found (and module API not available).\" );\n  return macro.execute({ itemUuid });\n})()\n",
          "altRollMessage": "",
          "rollMessageToChat": true,
          "altRollMessageToChat": true,
          "style": "label"
        }
